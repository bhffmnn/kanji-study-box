                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <HBox alignment="CENTER">
                           <children>
                              <Label fx:id="charLabel" alignment="TOP_CENTER" text="æ¼¢" textAlignment="CENTER">
